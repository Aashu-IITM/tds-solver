{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "server.py",
      "methods": ["POST"]
    },
    {
      "src": "/api/(.*)",
      "dest": "server.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "status": 404
    }
  ]
}
